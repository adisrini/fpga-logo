`timescale 1 ns/ 100 ps
module characterData_tb();

	reg [31:0] in;
	wire [31:0] out;
	reg [7:0] ps2_info;
	reg ps2_enable, clock, reset;
	
	characterData cd(out, in, ps2_info, clock, ps2_enable, reset);
	initial
	begin
		$display($time, "<<Starting the simulation>> ");
		$monitor("Reading the values of clock %b, input %b, ps2_info %b, and output %b", clock, in, ps2_info, out);
		#164480
		$stop;
	end
		always
			#20 opcodeA[0] = ~opcodeA[0];

endmodule